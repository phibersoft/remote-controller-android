<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_remote">

    <fragment
        android:id="@+id/navigation_remote"
        android:name="com.phibersoft.remoteph.ui.remote.RemoteFragment"
        android:label="@string/title_remote"
        tools:layout="@layout/fragment_remote" />

    <fragment
        android:id="@+id/navigation_lol"
        android:name="com.phibersoft.remoteph.ui.lol.LoLFragment"
        android:label="@string/title_lol"
        tools:layout="@layout/fragment_lol" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.phibersoft.remoteph.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>